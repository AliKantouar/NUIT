package com.example.ali.totoutard;


import android.app.Activity;
import android.content.Intent;
import android.graphics.pdf.PdfDocument;
import android.os.Bundle;
import android.support.v7.app.AppCompatActivity;
import android.view.View;
import android.widget.Button;
import android.widget.EditText;
import android.widget.TextView;

public class AddParticipant extends AppCompatActivity {

    private TextView nom;
    private TextView prenom;
    private EditText nomF;
    private EditText prenomF;
    private Button  button;


    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_add_participant);

        nom = (TextView) findViewById(R.id.nom);
        prenom = (TextView) findViewById(R.id.prenom);
        nomF = (EditText) findViewById(R.id.nomF);
        prenomF = (EditText) findViewById(R.id.prenomF);
        button = (Button) findViewById(R.id.button);

        button.setOnClickListener (new View.OnClickListener() {

            public void onClick(View v) {
                AjouterUnParticipant(nomF.getText().toString(),prenomF.getText().toString());
                Intent intent = new Intent (AddParticipant.this,AddParticipant.class);
                startActivity(intent);
            }
        });
    }

    protected void AjouterUnParticipant(String Nom ,String Prenom)
    {


    }

}
